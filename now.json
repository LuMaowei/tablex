{
    "name": "tablex",
    "version": 2,
    "builds": [
      {
        "src": "./packages/siteCN/package.json",
        "use": "@now/static-build",
        "config": {
          "includeFiles": "./packages/siteCN/doc/**",
          "distDir": "./.docz/dist"
